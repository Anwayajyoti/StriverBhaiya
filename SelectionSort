#Select MINIMUM and SWAP

>[13,46,24,52,20,9]
>[9,46,24,52,20,13]
>[9,13,24,52,20,46]
>[9,13,20,52,24,46]
>[9,13,20,24,52,46]
>[9,13,20,24,46,52]

#Takes n-1 Steps
 #CODE

arr=[13,46,24,52,20,9]

for i in range(len(arr)):
    min_index=i
    for j in range(i+1,len(arr)):
        if arr[j]<arr[min_index]:
            min_index=j

    (arr[i],arr[min_index])=(arr[min_index],arr[i])

for i in range(len(arr)):
    print(arr[i],end=' ')
